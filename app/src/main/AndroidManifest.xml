<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.devipriya.reportcard" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Subjects"
            android:label="@string/title_activity_subjects" >
        </activity>
        <activity
            android:name=".Tests"
            android:label="@string/title_activity_tests" >
        </activity>
        <activity
            android:name=".Marks"
            android:label="@string/title_activity_marks" >
        </activity>
        <activity
            android:name=".ReportCard"
            android:label="@string/title_activity_report_card" >
        </activity>
        <activity
            android:name=".UserSettings"
            android:label="@string/title_activity_user_settings" >
        </activity>
        <activity
            android:name=".Analyse"
            android:label="@string/title_activity_analyse" >
        </activity>
        <activity
            android:name=".Analysis_Test"
            android:label="@string/title_activity_analysis__test" >
        </activity>
        <activity
            android:name=".Analysis_Subject"
            android:label="@string/title_activity_analysis__subject" >
        </activity>
        <activity
            android:name=".GradePref"
            android:label="@string/title_activity_grade_pref" >
        </activity>
    </application>

</manifest>
